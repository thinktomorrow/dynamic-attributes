{
    "name": "thinktomorrow/dynamic-attributes",
    "description": "easily store and retrieve document values of an eloquent model",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ben Cavens",
            "email": "ben@thinktomorrow.be"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "illuminate/database": "^8.12",
        "doctrine/dbal": "^3.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "orchestra/testbench": "^6.18",
        "phpunit/phpunit": "^9.5",
        "psalm/plugin-laravel": "^1.4",
        "vimeo/psalm": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "Thinktomorrow\\DynamicAttributes\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Thinktomorrow\\DynamicAttributes\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
